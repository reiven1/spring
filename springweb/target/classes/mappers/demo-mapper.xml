<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="demo">
	<resultMap id="demoMap" type="demo">
		<result column="devlang" property="devLang"
			typeHandler="strArr" />
	</resultMap>
	<select id="selectAll" resultMap="demoMap" parameterType="map">
		SELECT
		* FROM (SELECT rownum AS rnum,d.* FROM (SELECT * FROM DEV ORDER BY
		DEV.DEVNAME) d)WHERE rnum BETWEEN #{start} AND #{end}
	</select>
	<insert id="insertDemo" parameterType="demo">
		INSERT INTO DEV
		VALUES(SEQ_DEV_NO.NEXTVAL,#{devName},#{devAge},#{devEmail},#{devGender},#{devLang,typeHandler=strArr})
	</insert>
	<select id="countDemo" resultType="int">
		SELECT COUNT(*) FROM DEV 
	</select>

</mapper>