<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- spring bean 을 등록하는 XML 파일 -->
	<!--기본 생성자로 생성해서 등록 -->
	<bean id="test" class="com.spring.test.TestClass" scope="prototype"/>
	<bean id="test2" class="com.spring.test.TestClass">
		<property name="name" value="강원준왜안와!"/>
	</bean>
	<bean id="test3" class="com.spring.test.TestClass">
		<constructor-arg index="0" value="전땅크 전화는 안 끝나?"/>
		<constructor-arg index="1" value="25"/>
	</bean>
</beans>